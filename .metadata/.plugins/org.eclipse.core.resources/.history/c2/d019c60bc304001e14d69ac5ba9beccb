package objects;

public class Student {

	String firstName;
	String lastName;
	String email;
	int age;
	String address;
	int batchNum;
	
	public void attendClass() {
		System.out.println("The student attended class");
	}
	
	public void setFirstName(String newFirstName) {
		firstName = newFirstName;
	}
	
	public String getFirstName() {
		return firstName;
	}
	
